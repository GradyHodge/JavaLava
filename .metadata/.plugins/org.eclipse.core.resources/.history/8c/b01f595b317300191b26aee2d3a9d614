package ttsClassroom;

public class May10Classroom {

	public static int drawCard (){
		
		Random rand = new Random();
		int cardOne = rand.nexInt(9) +2;
		return cardOne;
	}
	
	public static void sum(int playerCard1, int playerCard2) {
		int total1 = playerCard1 + playerCard2;
		System.out.println(total1 + "is the total");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int playerCard1 = drawCard();
		int playerCard1 = drawCard();
		
		sum(playerCard1, playerCard2)
	}

}
