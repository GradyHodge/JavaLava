package ttsHomeWork;
	
public class CarClassHmwk {

	////Step1 - create your class attributes
//	int id;
//	String model;
//	String make;
//	int yearMade;
//	boolean transformer;
	////Step2 - go to class with main method and create an object, or an instance of this class.

	////Setp3 - highlight one of the attributes such as "id".
	////Step4 - click on Source Tab in menu => click on "generate getters and setters"
	////Step5 - select which attributes and methods you want to make getters and setters.
	
	
		int id;
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		public String getModel() {
			return model;
		}
		public void setModel(String model) {
			this.model = model;
		}
		public String getMake() {
			return make;
		}
		public void setMake(String make) {
			this.make = make;
		}
		public int getYearMade() {
			return yearMade;
		}
		public void setYearMade(int yearMade) {
			this.yearMade = yearMade;
		}
		public boolean isTransformer() {
			return transformer;
		}
		public void setTransformer(boolean transformer) {
			this.transformer = transformer;
		}
		String model;
		String make;
		int yearMade;
		boolean transformer;
	}
