
public class May6_classPrac {
	
	public static void loop() {
		int x = 5;
			if (x == 4) {
				System.out.println("x is not equal to 5");
			} else {
				System.out.println("x IS equal to 5");
			}		
	}
	
	public static void kingKobra() {
		for (int i = 0; i < 10; i++);
			System.out.println("Funka Shawn");
	}
	
	public static void readMe1() {
		System.out.println("Het man, tell me something.");
		Scanner in = new Scanner(System.in);
		System.out.println("You just said, ") + in.nextLine());
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		loop();
		kingKobra();
	}

}
